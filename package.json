    "express": "~4.16.1",
    "http-errors": "^2.0.0",
    "jade": "~1.11.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "postgres": "^3.2.4",
    "sequelize": "^6.20.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.36",
    "@types/pg": "^8.6.5",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
